<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services https://symfony.com/schema/dic/services/services-1.0.xsd">
    <services>
        <service id="BitBag\ShopwareDpdApp\Api\PackageService">
            <argument type="service" id="BitBag\ShopwareDpdApp\Factory\PackageFactory" />
            <argument type="service" id="BitBag\ShopwareDpdApp\Resolver\ApiClientResolver" />
        </service>

        <service id="BitBag\ShopwareDpdApp\Api\OrderCourierService">
            <argument type="service" id="BitBag\ShopwareDpdApp\Factory\OrderCourier\PickupPayer" />
            <argument type="service" id="BitBag\ShopwareDpdApp\Factory\OrderCourier\PickupCustomer" />
            <argument type="service" id="BitBag\ShopwareDpdApp\Factory\OrderCourier\PickupSender" />
            <argument type="service" id="BitBag\ShopwareDpdApp\Factory\OrderCourier\PickupParameters" />
            <argument type="service" id="BitBag\ShopwareDpdApp\Factory\OrderCourier\PickupDetails" />
            <argument type="service" id="BitBag\ShopwareDpdApp\Factory\OrderCourier\DpdPickupParameters" />
            <argument type="service" id="doctrine.orm.entity_manager" />
            <argument type="service" id="translator" />
            <argument type="service" id="BitBag\ShopwareDpdApp\Resolver\ApiClientResolver" />
            <argument type="service" id="BitBag\ShopwareDpdApp\Repository\ConfigRepository" />
        </service>
    </services>
</container>
