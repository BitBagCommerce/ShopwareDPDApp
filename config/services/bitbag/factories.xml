<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services https://symfony.com/schema/dic/services/services-1.0.xsd">
    <services>
        <service id="BitBag\ShopwareDpdApp\Factory\DpdSenderFactory">
            <argument type="service" id="BitBag\ShopwareDpdApp\Repository\ConfigRepository" />
        </service>

        <service id="BitBag\ShopwareDpdApp\Factory\PackageFactory">
            <argument type="service" id="BitBag\ShopwareDpdApp\Factory\DpdSenderFactory" />
            <argument type="service" id="BitBag\ShopwareDpdApp\Factory\ParcelFactory" />
            <argument type="service" id="BitBag\ShopwareDpdApp\Factory\ReceiverFactory" />
        </service>

        <service id="BitBag\ShopwareDpdApp\Factory\ParcelFactory">
            <argument type="service" id="BitBag\ShopwareDpdApp\Resolver\OrderCustomFieldsResolver" />
            <argument type="service" id="BitBag\ShopwareDpdApp\Calculator\OrderWeightCalculator" />
        </service>

        <service id="BitBag\ShopwareDpdApp\Factory\ReceiverFactory" />

        <service id="BitBag\ShopwareDpdApp\Factory\ShippingMethodPayloadFactory" />

        <service id="BitBag\ShopwareDpdApp\Factory\RulePackageFactory" />

        <service id="BitBag\ShopwareDpdApp\Factory\FeedbackResponseFactory">
            <argument type="service" id="translator" />
        </service>

        <service id="BitBag\ShopwareDpdApp\Factory\OrderCourier\DpdPickupParametersFactory" />

        <service id="BitBag\ShopwareDpdApp\Factory\OrderCourier\PickupCustomerFactory" />

        <service id="BitBag\ShopwareDpdApp\Factory\OrderCourier\PickupDetailsFactory" />

        <service id="BitBag\ShopwareDpdApp\Factory\OrderCourier\PickupParametersFactory">
            <argument type="service" id="BitBag\ShopwareDpdApp\Resolver\OrderCustomFieldsResolver" />
            <argument type="service" id="BitBag\ShopwareDpdApp\Calculator\OrderWeightCalculator" />
        </service>

        <service id="BitBag\ShopwareDpdApp\Factory\OrderCourier\PickupSenderFactory" />

        <service id="BitBag\ShopwareDpdApp\Factory\OrderCourier\PickupPayerFactory" />

        <service id="BitBag\ShopwareDpdApp\Factory\OrderCourier\PackagePickupFactory">
            <argument type="service" id="BitBag\ShopwareDpdApp\Factory\OrderCourier\PickupPayerFactory" />
            <argument type="service" id="BitBag\ShopwareDpdApp\Factory\OrderCourier\PickupCustomerFactory" />
            <argument type="service" id="BitBag\ShopwareDpdApp\Factory\OrderCourier\PickupSenderFactory" />
            <argument type="service" id="BitBag\ShopwareDpdApp\Factory\OrderCourier\PickupParametersFactory" />
            <argument type="service" id="BitBag\ShopwareDpdApp\Factory\OrderCourier\PickupDetailsFactory" />
            <argument type="service" id="BitBag\ShopwareDpdApp\Factory\OrderCourier\DpdPickupParametersFactory" />
            <argument type="service" id="BitBag\ShopwareDpdApp\Repository\ConfigRepository" />
        </service>
    </services>
</container>
